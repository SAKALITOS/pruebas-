function addPlayerLabel(Tab, text)
	local Label = Instance.new("TextButton")
	local LabelCorner = Instance.new("UICorner")
	local LabelTitle = Instance.new("TextLabel")

	Label.Name = "Button"
	Label.Parent = Tab
	Label.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
	Label.Size = UDim2.new(0, 292, 0, 26)
	Label.AutoButtonColor = false
	Label.Font = Enum.Font.Gotham
	Label.Text = "	"..text
	Label.TextColor3 = Color3.fromRGB(255, 0, 4)
	Label.TextSize = 16.000

	LabelCorner.CornerRadius = UDim.new(0, 5)
	LabelCorner.Name = "ButtonCorner"
	LabelCorner.Parent = Label


	Tab.CanvasSize = UDim2.new(0, 0, 0, UIListLayoutPlayer.AbsoluteContentSize.Y)
end
