--Funcion Del Frame
game:GetService("UserInputService").InputBegan:Connect(function(input,chatting)
	if input.UserInputType == Enum.UserInputType.Keyboard then
		if chatting then
			return
		end
		if input.KeyCode == Enum.KeyCode.P then
			ScreenGuiSalakitos.Enabled = not ScreenGuiSalakitos.Enabled
		end
	end
end)
--Funcion para que se pueda
function dragify(Frame)
	local UIS = game:GetService("UserInputService")
	dragToggle = nil
	local dragSpeed = 0
	dragInput = nil
	dragStart = nil
	local dragPos = nil
	function updateInput(input)
		local Delta = input.Position - dragStart
		local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
		game:GetService("TweenService"):Create(SalakitosFrame, TweenInfo.new(0.3), {Position = Position}):Play()
	end
	SalakitosFrame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
			dragToggle = true
			dragStart = input.Position
			startPos = Frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	SalakitosFrame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragToggle then
			updateInput(input)
		end
	end)
end
dragify(SalakitosFrame)
--[[
 _____ _     _     ______           ______       _   _                  
/  ___(_)   | |    | ___ \          | ___ \     | | | |                 
\ `--. _  __| | ___| |_/ / __ _ _ __| |_/ /_   _| |_| |_ ___  _ __  ___ 
 `--. \ |/ _` |/ _ \ ___ \/ _` | '__| ___ \ | | | __| __/ _ \| '_ \/ __|
/\__/ / | (_| |  __/ |_/ / (_| | |  | |_/ / |_| | |_| || (_) | | | \__ \
\____/|_|\__,_|\___\____/ \__,_|_|  \____/ \__,_|\__|\__\___/|_| |_|___/
                                                                                                                                              
--]]
function addSideButton(Tab, text, callback)
	local Button = Instance.new("TextButton")
	local ButtonCorner = Instance.new("UICorner")

	Button.Name = "Button"
	Button.Parent = Tab
	Button.BackgroundColor3 = Color3.fromRGB(14, 14, 14)
	Button.Size = UDim2.new(0, 118, 0, 40)
	Button.AutoButtonColor = false
	Button.Font = Enum.Font.GothamBold
	Button.Text = text
	Button.TextColor3 = Color3.fromRGB(255, 0, 4)
	Button.TextSize = 16.000
	Button.TextXAlignment = Enum.TextXAlignment.Center
	Button.BackgroundTransparency = 0.950

	ButtonCorner.CornerRadius = UDim.new(0, 0)
	ButtonCorner.Name = "ButtonCorner"
	ButtonCorner.Parent = Button

	Button.MouseEnter:Connect(
		function()
			TweenService:Create(
				Button,
				TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{BackgroundColor3 = Color3.fromRGB(17, 17, 17)}
			):Play()
		end
	)

	Button.MouseLeave:Connect(
		function()
			TweenService:Create(
				Button,
				TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{BackgroundColor3 = Color3.fromRGB(14, 14, 14)}
			):Play()
		end
	)

	Button.MouseButton1Click:Connect(
		function()
			pcall(callback)
		end
	)

	Tab.CanvasSize = UDim2.new(0, 0, 0, UILIstLayoutSide.AbsoluteContentSize.Y)
end
function GetFramesVisible()
	HouseFrame.Visible = false
	ObjetsFrame.Visible = false
	OthersFrame.Visible = false
	PlayerFrame.Visible = false
	TortureFrame.Visible = false
	VehicleFrame.Visible = false
	WaypointsFrame.Visible = false
	WelcomeFrame.Visible = false
end
addSideButton(ScrollingFrameSide, "Home", function()
	GetFramesVisible()
	WelcomeFrame.Visible = true
end)
addSideButton(ScrollingFrameSide, "Player", function()
	GetFramesVisible()
	PlayerFrame.Visible = true
end)
addSideButton(ScrollingFrameSide, "Vehicle", function()
	GetFramesVisible()
	VehicleFrame.Visible = true
end)
addSideButton(ScrollingFrameSide, "Objects", function()
	GetFramesVisible()
	ObjetsFrame.Visible = true
end)
addSideButton(ScrollingFrameSide, "Waypoints", function()
	GetFramesVisible()
	WaypointsFrame.Visible = true
end)
addSideButton(ScrollingFrameSide, "Torture", function()
	GetFramesVisible()
	TortureFrame.Visible = true
end)
addSideButton(ScrollingFrameSide, "House", function()
	GetFramesVisible()
	HouseFrame.Visible = true
end)
addSideButton(ScrollingFrameSide, "Others", function()
	GetFramesVisible()
	OthersFrame.Visible = true
end)
